{
  "atomKeymap.promptV3Features": true,
  "auto-close-tag.activationOnLanguage": [
    "html",
    "xml",
    "javascriptreact",
    "typescriptreact"
  ],
  "bazel.buildifierFixOnFormat": true,
  "breadcrumbs.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "'Source Code Pro', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    "Vagrantfile": "ruby",
    "Dockerfile.dev": "docker",
    "*.ejs": "ejs",
    ".prototools": "toml",
    ".prototools.*": "toml"
  },
  "files.autoGuessEncoding": false,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.checkoutType": ["local"],
  "go.formatOnSave": true,
  "go.formatTool": "gofmt",
  "go.lintOnSave": "file",
  "html.format.endWithNewline": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "javascript.validate.enable": true,
  "jsDocCompletion.enabled": false,
  "material-icon-theme.files.associations": {
    "story.ts": "storybook",
    "story.tsx": "storybook"
  },
  "search.followSymlinks": false,
  "todohighlight.keywords": ["TODO", "FIXME"],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.enableWindowsBackgroundUpdates": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.visible": true,
  "[ejs]": {
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true
  },
  "rust-analyzer.hover.actions.run.enable": false,
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false
}
